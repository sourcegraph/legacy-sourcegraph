    "directory": "client/extension-api-types"
  },
  "license": "Apache-2.0",
  "main": "",
  "types": "src/index.d.ts",
  "files": [
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "lint:js": "pnpm eslint 'src/**/*.[jt]s?(x)'",
